{
  "mcpServers": {
    "laravel-boost": {
      "command": "powershell.exe",
      "args": [
        "-ExecutionPolicy",
        "Bypass",
        "-File",
        ".\\artisan.ps1",
        "boost:mcp"
      ],
      "cwd": "D:\\DevCenter\\abuilds\\alumate",
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": [
        "application_info",
        "database_schema",
        "list_routes",
        "get_config",
        "search_docs",
        "list_artisan_commands",
        "tinker",
        "read_log_entries",
        "last_error"
      ]
    }
  }
}