{
    "name": "alumni-platform/api-client",
    "description": "Official PHP SDK for the Alumni Platform API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alumni Platform Team",
            "email": "developers@alumni-platform.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "AlumniPlatform\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlumniPlatform\\ApiClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlumniPlatform\\ApiClient\\AlumniPlatformServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}